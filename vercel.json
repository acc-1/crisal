{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/formdata",
            "methods": ["POST", "OPTIONS"],
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
